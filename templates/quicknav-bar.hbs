<div class="block-inner quicknav-inner">

	<button id="previous" class="button{{#if config._buttons._previous._isHidden}} display-none{{/if}}{{#if state.isFirstPage}} disabled{{/if}}" {{#if state.isFirstPage}} disabled="disabled" {{/if}} aria-label="{{config._buttons._previous.text}}">
		<div class='icon icon-controls-left' />
		<div class='previous-inner'>
			<div class='prompt'>Previous</div>
			<div class='button-inner'>
				{{config._buttons._previous.text}}
			</div>
		</div>
	</button>

	<div class='seperator' />

	<button id="next" class="button{{#if config._buttons._next._isHidden}} display-none{{/if}}{{#if state.isLastPage}} disabled{{/if}}" {{#if state.isLastPage}} disabled="disabled" {{/if}} aria-label="{{config._buttons._next.text}}">
		<div class='next-inner'>
	  		<div class='prompt'>Next</div>
	  		<div class='button-inner'>
		  		{{config._buttons._next.text}}
	      	</div>
      	</div>
      	<div class='icon icon-controls-right' />
	</button>
</div>
